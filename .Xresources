!------------------------------
! XFT settings
!------------------------------
Xft.dpi:                96
Xft.antialias:          true
Xft.rgba:               rgb
Xft.hinting:            true
Xft.autohint:           false
Xft.hintstyle:          hintslight
Xft.lcdfilter:          lcddefault

!------------------------------
! URxvt settings
!------------------------------
URxvt.scrollBar:        false
URxvt.saveLines:        10000
URxvt.cursorUnderline:  false
URxvt.letterspace:      -1
URxvt.depth:            32
URxvt.cursorBlink:      true
URxvt.geometry:         80x20

!------------------------------
! URxvt fonts
!------------------------------
!URxvt*font: xft:berry:size=8, \
!            xft:Kochi Gothic:antialias=false
!
URxvt*font: -xos4-terminesspowerline-medium-r-normal--12-120-72-72-c-60-iso10646-1
URxvt*boldFont:


!------------------------------
! Color scheme
!------------------------------
#include "./.colors/navy-ivory"

!------------------------------
! Rofi settings
!------------------------------
rofi.color-enabled: true
rofi.color-window: #1D1F21, #1D1F21, #121519
rofi.color-normal: #1D1F21, #f9f9f9, #1D1F21, #F22437, #f9f9f9
rofi.color-active: #1D1F21, #555577, #1D1F21, #555577, #1D1F21
rofi.color-urgent: #1D1F21, #ee6644, #1D1F21, #ee6644, #1D1F21
rofi.lines: 15
rofi.width: 100%
rofi.font: M+ 1m medium 8
rofi.yoffset: 14
rofi.xoffset: 0
rofi.location: 1
rofi.opacity: 85
