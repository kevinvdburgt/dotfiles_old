!------------------------------
! XFT settings
!------------------------------
Xft.dpi:                96
Xft.antialias:          true
Xft.rgba:               rgb
Xft.hinting:            true
Xft.autohint:           false
Xft.hintstyle:          hintslight
Xft.lcdfilter:          lcddefault

!------------------------------
! URxvt settings
!------------------------------
URxvt.scrollBar:        false
URxvt.saveLines:        10000
URxvt.cursorUnderline:  false
URxvt.letterspace:      -1
URxvt.depth:            32
URxvt.cursorBlink:      true
URxvt.geometry:         80x20

!------------------------------
! URxvt fonts
!------------------------------
!URxvt*font: xft:berry:size=8, \
!            xft:Kochi Gothic:antialias=false
!
URxvt*font: -xos4-terminesspowerline-medium-r-normal--12-120-72-72-c-60-iso10646-1
URxvt*boldFont:


